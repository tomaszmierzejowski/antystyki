{
  "ConnectionStrings": {
    "DefaultConnection": "Host=localhost;Port=5432;Database=antystics;Username=postgres;Password=PLACEHOLDER"
  },
  "Jwt": {
    "Secret": "PLACEHOLDER-USE-ENVIRONMENT-VARIABLES-IN-PRODUCTION",
    "Issuer": "Antystics",
    "Audience": "AntysticsUsers"
  },
  "Email": {
    "SmtpHost": "smtp.gmail.com",
    "SmtpPort": "587",
    "SmtpUser": "PLACEHOLDER-USE-ENVIRONMENT-VARIABLES",
    "SmtpPassword": "PLACEHOLDER-USE-ENVIRONMENT-VARIABLES",
    "FromAddress": "noreply@antystyki.pl",
    "FromName": "Antystyki"
  },
  "Storage": {
    "LocalPath": "wwwroot/uploads"
  },
  "FrontendUrl": "http://localhost:5173",
  "Authentication": {
    "Google": {
      "ClientId": "",
      "ClientSecret": ""
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Warning"
      }
    },
    "Enrich": [ "FromLogContext" ],
    "Properties": {
      "service": "antystics-api"
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "formatter": {
            "type": "Serilog.Formatting.Compact.CompactJsonFormatter, Serilog.Formatting.Compact"
          }
        }
      }
    ]
  },
  "Sentry": {
    "Dsn": "",
    "Enabled": false,
    "Environment": "Production",
    "Release": "antystics@1.0.0",
    "SendDefaultPii": false,
    "TracesSampleRate": 0.0
  },
  "VisitorMetrics": {
    "RetentionDays": 60,
    "FlushIntervalMinutes": 5,
    "TrackBots": true
  },
  "AllowedHosts": "*"
}
